prefix=${pcfiledir}/../..
exec_prefix=${prefix}
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=${prefix}/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.5.2
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lpthread
Cflags: -DDARWIN -DSIGPROCMASK_SETS_THREAD_MASK  -I${includedir}
