prefix=/usr/local/Cellar/fontconfig/2.17.1
includedir=${prefix}/include
libdir=${prefix}/lib

sysconfdir=/usr/local/etc
localstatedir=/usr/local/var
confdir=/usr/local/etc/fonts
cachedir=/usr/local/var/cache/fontconfig

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.17.1
Requires.private: freetype2 >=  21.0.15, expat
Libs: -L${libdir} -lfontconfig
Libs.private: -lintl -lm
Cflags: -I${includedir}
